{
    "title": "码良的笔记本",
    "author": "Anani",
    "description": "码良的笔记本，记录学习中的点点滴滴。",
    "language": "zh-hans",
    "direction": "ltr",
    "gitbook": ">= 3.2.3",
    "links": {
        "sidebar": {
            "博客": "https://anani1994.github.io/blog/#/"
        }
    },
    "plugins": [
        "custom-favicon",
        "chapter-fold",
        "splitter",
        "-sharing",
        "sharing-plus",
        "back-to-top-button",
        "code",
        "-highlight",
        "prism",
        "github",
        "edit-link",
        "page-copyright"
    ],
    "pluginsConfig": {
        "favicon": "./src/images/favicon.ico",
        "sharing": {
            "all": [
                "facebook",
                "twitter",
                "douban",
                "qq",
                "weibo",
                "qzone",
                "google",
                "instapaper"
            ]
        },
        "github": {
            "url": "https://github.com/Anani1994/market"
        },
        "edit-link": {
            "base": "https://github.com/Anani1994/notebook/edit/master/",
            "label": "Edit This Page"
        },
        "page-copyright": {
            "signature": "Anani",
            "description": "modified at",
            "wisdom": "More than a coder, more than a designer.",
            "format": "yyyy-MM-dd hh:mm:ss",
            "copyright": "Copyright &#169; Anani",
            "timeColor": "#666",
            "copyrightColor": "#666",
            "utcOffset": "8",
            "isShowQRCode": true,
            "baseUri": "https://anani1994.github.io/notebook/",
            "isShowIssues": true,
            "repo": "notebook",
            "token": null,
            "style": "normal",
            "noPowered": true
        }
    }
}